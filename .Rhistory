complete <- function(directory, id = 1:332) {
##setup directory where files are held
dataset <- list.files(directory, pattern = ".csv$", full.names = TRUE)
##creating the id vector
id <- id
##create the base data frames
completedataset <- data.frame()
subset_completedataset <- data.frame()
nobsdata <- data.frame()
nobssum <- data.frame()
nobs <- data.frame()
##setting up a for loop to read each data file at a time
for(i in dataset) {
completedataset <- rbind(completedataset, read.csv(dataset[i]))
##excluding the NA values
subset_completedataset <- na.omit(completedataset)
##count the number of rows
nobsdata <- complete.cases(subset_completedataset)
##sum of the rows
nobssum <- sum(nobsdata)
##create the output
nobs <- data.frame(id = id, nobssum = nobssum)
}
}
